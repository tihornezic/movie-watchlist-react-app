ched list\r\n                watched: [action.payload, ...state.watched]\r\n            }\r\n        case 'MOVE_TO_WATCHLIST':\r\n            return {\r\n                ...state,\r\n                watched: state.watched.filter(movie => movie.id !== action.payload.id),\r\n                watchlist: [action.payload, ...state.watchlist]\r\n            }\r\n        case 'REMOVE_FROM_WATCHED':\r\n            return {\r\n                ...state,\r\n                watched: state.watched.filter(movie => movie.id !== action.payload)\r\n            }\r\n        default:\r\n            return state;\r\n    }\r\n}","C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\MovieCard.js",[],"C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\MovieControls.js",["59"],"C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\Modal.js",[],{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":8,"column":1,"nodeType":"66","endLine":44,"endColumn":2},{"ruleId":"67","severity":1,"message":"68","line":4,"column":16,"nodeType":"69","messageId":"70","endLine":4,"endColumn":24},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-unused-vars","'toastify' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]