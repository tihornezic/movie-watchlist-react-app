O_WATCHED':\r\n            return {\r\n                ...state,\r\n                // remove a movie from the watchlist\r\n                watchlist: state.watchlist.filter(movie => movie.id !== action.payload.id),\r\n                // and then add it to the watched list\r\n                watched: [action.payload, ...state.watched]\r\n            }\r\n        case 'MOVE_TO_WATCHLIST':\r\n            return {\r\n                ...state,\r\n                watched: state.watched.filter(movie => movie.id !== action.payload.id),\r\n                watchlist: [action.payload, ...state.watchlist]\r\n            }\r\n        case 'REMOVE_FROM_WATCHED':\r\n            return {\r\n                ...state,\r\n                watched: state.watched.filter(movie => movie.id !== action.payload)\r\n            }\r\n        default:\r\n            return state;\r\n    }\r\n}","C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\MovieCard.js",[],"C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\MovieControls.js",["63"],"C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\Modal.js",["64"],"C:\\Users\\Tihor\\Documents\\Web dev\\movie-watchlist-react\\src\\components\\ModalMovieControls.js",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":8,"column":1,"nodeType":"72","endLine":44,"endColumn":2},{"ruleId":"73","severity":1,"message":"74","line":4,"column":16,"nodeType":"75","messageId":"76","endLine":4,"endColumn":24},{"ruleId":"73","severity":1,"message":"77","line":2,"column":9,"nodeType":"75","messageId":"76","endLine":2,"endColumn":16},{"ruleId":"73","severity":1,"message":"74","line":4,"column":16,"nodeType":"75","messageId":"76","endLine":4,"endColumn":24},"no-native-reassign",["78"],"no-negated-in-lhs",["79"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","E